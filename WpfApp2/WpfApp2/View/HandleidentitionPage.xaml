<UserControl x:Class="WpfApp2.View.HandleidentitionPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2.View"
        mc:Ignorable="d"
          d:DesignHeight="1024" d:DesignWidth="1280"
   
        Background="Transparent" OpacityMask="White"
        ScrollViewer.VerticalScrollBarVisibility="Hidden" Loaded="Window_Loaded">
    <Viewbox Stretch="Fill">
        <Grid>
            <Label Panel.ZIndex="2" Name="actionName" Foreground="White" FontSize="35" Height="50" HorizontalAlignment="Center" Margin="0,35,0,0" VerticalAlignment="Top" >办  卡</Label>
            <Label Name="Time" Foreground="#FFCB2D" Panel.ZIndex="2" HorizontalAlignment="Right" FontSize="25" Margin="50"></Label>
            <Image  Source="../ControlImages/底图.png" Width="1280" Height="1024" Stretch="Fill"/>
            <Grid Name="GRID">

        </Grid>
    </Grid>
    </Viewbox>
</UserControl>